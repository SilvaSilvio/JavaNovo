package Arrays;

public class Arrays {

	public static void main(String[] args) {
		int idade1 = 12;
		int idade2 = 45;
		int idade3 = 54;
		
		int[] idades = new int[6];
		
		idades[0] = 12;
		idades[1] = 2;
		idades[2] = 3;
		idades[3] = 4;
		idades[4] = 5;
		idades[5] = 6;
		
		System.out.println(idades[5]);
		

	}

}
